generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int      @id @default(autoincrement())
  name          String 
  username String? @unique
  organization  String
  job           String
  location      String
  email         String   @unique
  password      String
  createdAt     DateTime @default(now())
}
